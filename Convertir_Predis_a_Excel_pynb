        "\n",
        "from google.colab import files\n",
        "\n",
        "# Subir archivo PDF desde tu PC\n",
        "uploaded = files.upload()\n",
        "\n",
        "# Tomar el primer archivo subido\n",
        "pdf_path = list(uploaded.keys())[0]\n",
        "\n",
        "# Abrir el PDF\n",
        "doc = fitz.open(pdf_path)\n",
