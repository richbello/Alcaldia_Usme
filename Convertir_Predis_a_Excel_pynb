        "# Extraer texto\n",
        "texto = \"\"\n",
        "for pagina in doc:\n",
        "    texto += pagina.get_text(\"text\")\n",
        "\n",
        "# Mostrar los primeros 1000 caracteres\n",
        "print(texto[:1000])\n"
